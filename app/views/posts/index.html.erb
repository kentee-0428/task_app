<h2>スケージュール一覧</h2>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>タイトル</th>
      <th>開始日</th>
      <th>終了日</th>
      <th>終日</th>
      <th>スケジュール更新日</th>
      <th>確認</th>
      <th>編集</th>
      <th>削除</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.id %></td>
        <td></td>
        <td></td>
        <td></td>
        <td><%= post.content %></td>
        <td><%= post.updated_at %></td>
        <td><%= link_to("確認", "/posts/#{post.id}") %></td>
        <td><%= link_to("編集", "/posts/#{post.id}/edit") %></td>
        <td><%= link_to("削除", "/posts/#{post.id}/destroy", {method:'post'}) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to("スケジュール新規登録", "/posts/new") %>

